package task9new;

public class Q4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char a = '*';
		  
		System.out.print("   ");
		  System.out.print(a);
		  System.out.print(" ");
		  System.out.println(a);
		char b = '*';
		  
		System.out.print("   ");
		System.out.print(b);
		System.out.println(b);
		
		 a = '*';
		 System.out.print("    ");
		 System.out.println(a);
		 
		 b = '*';
		  
		 System.out.print("   ");
			System.out.print(b);
			System.out.println(b);
			
			a = '*';
			  
			System.out.print("   ");
			  System.out.print(a);
			  System.out.print(" ");
			  System.out.println(a);
			
		 
		  
		
		
		
	}


	}


