package task9new;


public class Q6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int month=3;
		int days=2;
		float rent=1500;
        double  t;
		
		switch(month) {
		case 1:
			t=rent*days;
		System.out.println( t);
		break;
		case 2:
			t=rent*days;
		System.out.println( t);
		break;
		case 3:
			t=rent*days;
		System.out.println(t);
		break;
		case 4:
			t=((rent+(rent*0.2))*days);
		System.out.println( t);
		break;
		case 5:
			t=((rent+(rent*0.2))*days);
			System.out.println( t);
		break;
		case 6:
			t=((rent+(rent*0.2))*days);
		System.out.println( t);
		break;
		case 7:
			t=rent*days;
		System.out.println( t);
		break;
		case 8:
			t=rent*days;
		System.out.println( t);
		break;
		case 9:
			t=rent*days;
		System.out.println( t);
		break;
		case 10:
			t=rent*days;
		System.out.println( t);
		break;
		case 11:
			t=((rent+(rent*0.2))*days);
		System.out.println( t);
		break;
		case 12:
			t=((rent+(rent*0.2))*days);
		System.out.println( t);
		break;
			
		 default :
   	    System.out.println("invalid input");
   		  break;

		}}}


