public class Q3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int num=1;
int i,j;

for(i=1;i<=4;i++)
{
	for (j=1;j<=i;j++)
	{
		System.out.print (num +" ");
		num++;
	}
			System.out.println();
		
	
